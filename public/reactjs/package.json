{
  "name": "loan-dashboard",
  "version": "1.0.0",
  "author": " Tech",
  "license": "",
  "description": "loan dashboard",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && rm -rf ../dist_react && mv build ../dist_react",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "install:clean-server": "rm -rf node_modules/ && rm -rf package-lock.json && npm install",
    "compile:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/argon-dashboard-pro-material-ui.scss src/assets/css/argon-dashboard-pro-material-ui.css",
    "minify:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/argon-dashboard-pro-material-ui.scss src/assets/css/argon-dashboard-pro-material-ui.min.css --output-style compressed",
    "map:scss": "node-sass --importer node_modules/node-sass-package-importer/dist/cli.js src/assets/scss/argon-dashboard-pro-material-ui.scss src/assets/css/argon-dashboard-pro-material-ui.css --source-map true",
    "all:scss": "npm run compile:scss && npm run minify:scss && npm run map:scss"
  },
  "dependencies": {
    "@emotion/react": "11.1.5",
    "@emotion/styled": "11.1.5",
    "@fortawesome/fontawesome-free": "5.15.2",
    "@fullcalendar/core": "5.5.1",
    "@fullcalendar/daygrid": "5.5.0",
    "@fullcalendar/interaction": "5.5.0",
    "@material-ui/core": "4.11.3",
    "@material-ui/data-grid": "4.0.0-alpha.22",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.57",
    "@mui/icons-material": "5.2.0",
    "@mui/lab": "^5.0.0-alpha.68",
    "@mui/material": "5.2.0",
    "@mui/styled-engine": "5.2.0",
    "@mui/x-date-pickers": "^5.0.0-beta.2",
    "@sentry/browser": "^7.50.0",
    "@sentry/react": "^7.50.0",
    "@sentry/tracing": "^7.50.0",
    "axios": "^0.27.2",
    "bootstrap": "4.6.0",
    "chart.js": "2.9.4",
    "clsx": "1.1.1",
    "country-state-city": "^3.1.1",
    "date-fns": "^2.29.1",
    "dayjs": "^1.11.7",
    "dexie": "^3.2.4",
    "dexie-react-hooks": "^1.1.6",
    "dropzone": "5.7.6",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.25",
    "jss-rtl": "0.3.0",
    "list.js": "2.3.1",
    "material-ui-dropzone": "^3.5.0",
    "moment": "2.29.1",
    "node-sass": "5.0.0",
    "node-sass-package-importer": "5.3.2",
    "nouislider": "14.6.3",
    "prop-types": "15.7.2",
    "quill": "1.3.7",
    "react": "17.0.1",
    "react-bootstrap-sweetalert": "5.2.0",
    "react-bootstrap-table-next": "4.0.3",
    "react-bootstrap-table2-paginator": "2.1.2",
    "react-bootstrap-table2-toolkit": "2.1.3",
    "react-chartjs-2": "2.11.1",
    "react-copy-to-clipboard": "5.0.3",
    "react-csv": "^2.2.2",
    "react-datetime": "3.0.4",
    "react-dom": "17.0.1",
    "react-ga4": "^2.1.0",
    "react-jvectormap": "0.0.16",
    "react-perfect-scrollbar": "1.5.8",
    "react-redux": "^7.2.6",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-sdk": "git+https://git-codecommit.ap-south-1.amazonaws.com/v1/repos/react-sdk#v1.4.0",
    "react-to-print": "2.12.3",
    "redux": "^4.1.2",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "xls-to-json": "^2.1.0",
    "yarn": "^1.22.19"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "plugins": [
      "unused-imports"
    ],
    "rules": {
      "no-unused-vars": "off",
      "unused-imports/no-unused-imports": "error",
      "unused-imports/no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "varsIgnorePattern": "^_",
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    }
  },
  "optionalDependencies": {
    "popper.js": "1.16.1",
    "typescript": "4.1.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "7.20.0",
    "eslint-plugin-unused-imports": "1.1.0",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "saas": "^1.0.0",
    "react-error-overlay": "6.0.9"
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  }
}
